--TAL #4
SELECT CUSTOMER_NUM, CUSTOMER_NAME
  FROM CUSTOMER
 WHERE CUSTOMER_NUM IN (
  SELECT CUSTOMER_NUM
    FROM ORDERS
   WHERE ORDER_DATE = '15-OCT-15'
);

-- TAL #6
SELECT CUSTOMER_NUM, CUSTOMER_NAME
  FROM CUSTOMER
 WHERE CUSTOMER_NUM NOT IN (
  SELECT CUSTOMER_NUM
    FROM ORDERS
   WHERE ORDER_DATE = '15-OCT-15'
);

-- TAL #12
SELECT ITEM_NUM, DESCRIPTION, CATEGORY
  FROM ITEM
 ORDER BY CATEGORY;

-- TAL #14
SELECT ORDERS.ORDER_NUM, ORDERS.ORDER_DATE
  FROM ORDERS, ORDER_LINE, ITEM
 WHERE ORDERS.ORDER_NUM = ORDER_LINE.ORDER_NUM
   AND ORDER_LINE.ITEM_NUM = ITEM.ITEM_NUM
   AND ITEM.DESCRIPTION = 'Fire Engine';

-- COL #4
SELECT TRIP_NAME, TYPE
  FROM TRIP
 WHERE TYPE = 'Biking'
   AND TRIP_ID = ANY (
  SELECT TRIP_GUIDES.TRIP_ID
    FROM TRIP_GUIDES, GUIDE
   WHERE TRIP_GUIDES.GUIDE_NUM = GUIDE.GUIDE_NUM
     AND GUIDE.FIRST_NAME = 'Rita'
     AND GUIDE.LAST_NAME = 'Boyers'
);


-- COL #6
SELECT RESERVATION_ID, TRIP_ID, TRIP_DATE
  FROM RESERVATION
  WHERE TRIP_ID IN (
    SELECT TRIP_ID
      FROM TRIP
     WHERE STATE = 'ME'
);

-- COL #12
SELECT CUSTOMER_NUMBER_NUM, FIRST_NAME, LAST_NAME
  FROM CUSTOMERS
 WHERE STATE = 'NJ'
    OR CUSTOMER_NUMBER_NUM = ANY (
   SELECT CUSTOMER_NUM
     FROM RESERVATION
);

-- COL #14
SELECT TRIP_ID, TRIP_NAME
  FROM TRIP
 WHERE TYPE != 'Hiking'
   AND MAX_GRP_SIZE > ANY (
  SELECT MAX_GRP_SIZE
    FROM TRIP
   WHERE TYPE = 'Hiking'
);

-- COL #16
SELECT TRIP.TRIP_ID, TRIP.TRIP_NAME, RESERVATION.RESERVATION_ID
   FROM TRIP, RESERVATION
  WHERE TRIP.TRIP_ID = RESERVATION.TRIP_ID (+)
  ORDER BY TRIP_ID;

